{
  "name": "fshpal-language-support",
  "displayName": "FSHPAL Language Support",
  "description": "adds simple syntax highlighting for FSHPAL",
  "version": "1.0.0",
  "publisher": "streebs",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/streebs/fshpal-language-support"
  },
  "icon": "./logos/logo.png",
  "contributes": {
    "languages": [{
      "id": "fshpal",
      "aliases": ["FSHPAL", "fshpal"],
      "extensions": [".BAS", ".FIL", ".INC", ".BK!"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./logos/logo-small.png",
        "dark": "./logos/logo-small.png"
      }
    }],
    "grammars": [{
      "language": "fshpal",
      "scopeName": "source.BAS",
      "path": "./syntaxes/fshpal.tmLanguage.json"
    }]
  }
}
